{
  "name": "xmtp-gateway-console",
  "description": "Learn how XMTP messaging fees work by using them",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "typecheck": "tsc -b",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:e2e": "node tests/e2e/test-runner.js",
    "test:e2e:verbose": "node tests/e2e/test-runner.js --verbose",
    "test:e2e:wallet": "node tests/e2e/test-runner.js --with-wallet --verbose",
    "test:e2e:shell": "bash tests/e2e/run-tests.sh",
    "test:e2e:rainbowkit": "node tests/e2e/test-runner.js --with-wallet --test rainbowkit-modal --verbose",
    "test:e2e:multi-wallet": "node tests/e2e/test-runner.js --with-wallet --test multi-wallet-inject --verbose"
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "^4.3.7",
    "@mantine/hooks": "^8.3.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@rainbow-me/rainbowkit": "^2.2.10",
    "@tanstack/react-query": "^5.90.16",
    "@xmtp/browser-sdk": "^6.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2",
    "viem": "^2.44.2",
    "wagmi": "^2.19.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.8",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "shadcn": "^3.6.3",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^5.4.21"
  }
}
