# App name - displayed in the gateway console header
VITE_APP_NAME=MyApp

# XMTP contracts environment (testnet-staging, testnet, mainnet)
# Used by both frontend (VITE_) and gateway service (XMTPD_)
VITE_CONTRACTS_ENVIRONMENT=testnet-staging
XMTPD_CONTRACTS_ENVIRONMENT=testnet-staging

# Gateway URL - XMTP Gateway service (gRPC)
VITE_GATEWAY_URL=http://localhost:5050

# Gateway health check URL (optional - defaults to port 5051)
# VITE_GATEWAY_HEALTH_URL=http://localhost:5051/health

# Gateway payer address - the address that pays for messages
# Derived from PAYER_PRIVATE_KEY in the gateway service
VITE_GATEWAY_PAYER_ADDRESS=0x...

# Base Sepolia RPC (Settlement Chain)
VITE_SETTLEMENT_CHAIN_RPC_URL=https://sepolia.base.org

# Ethereum Mainnet RPC (for ENS resolution)
VITE_MAINNET_RPC_URL=https://eth.llamarpc.com

# WalletConnect Project ID (optional, for WalletConnect support)
VITE_WALLETCONNECT_PROJECT_ID=

# ---- Gateway Service (docker-compose) ----
# Private key for the payer wallet
# Generate with: cast wallet new
XMTPD_PAYER_PRIVATE_KEY=0x...

# XMTP App Chain (get from Alchemy)
XMTPD_APP_CHAIN_RPC_URL=https://xmtp-testnet.g.alchemy.com/public
XMTPD_APP_CHAIN_WSS_URL=wss://xmtp-testnet.g.alchemy.com/public

# Settlement Chain - Base Sepolia (get from Alchemy)
XMTPD_SETTLEMENT_CHAIN_RPC_URL=https://base-sepolia.g.alchemy.com/v2/YOUR_KEY
XMTPD_SETTLEMENT_CHAIN_WSS_URL=wss://base-sepolia.g.alchemy.com/v2/YOUR_KEY
